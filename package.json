{
  "name": "staircase",
  "version": "0.1.0",
  "description": "A collection of steps making it easy to build a static website.",
  "main": "build/index.js",
  "dependencies": {
    "babel-runtime": "^6.9.2",
    "bunyan": "^1.8.1",
    "filepath": "^1.1.0",
    "fs-promise": "^0.5.0",
    "marked": "^0.3.5",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "request-promise": "^3.0.0",
    "yaml-front-matter": "^3.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-eslint": "^6.0.3",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-flow-strip-types": "^6.7.0",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.7.2",
    "blue-tape": "^0.2.0",
    "eslint": "^2.8.0",
    "eslint-plugin-flowtype": "^2.2.7",
    "eslint-plugin-react": "^5.0.1",
    "flow-bin": "^0.23.1",
    "flow-copy-source": "^1.0.2",
    "glob": "^7.0.3",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.3",
    "mock-fs": "^3.8.0",
    "proxyquire": "^1.7.9",
    "sinon": "^1.17.3",
    "tape": "^4.5.1"
  },
  "scripts": {
    "all": "npm run build && npm run test && npm run flow && npm run lint",
    "babel": "babel src -d build",
    "build": "npm run babel && npm run flow-copy-source",
    "coverage": "isparta cover --root src --include-all-sources --report html testHelper.js",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "flow-copy-source": "flow-copy-source src build",
    "lint": "eslint src tst",
    "lint:fix": "eslint --fix src tst",
    "test": "node testHelper.js"
  },
  "author": "Matt Benton <m@mattb.tech>",
  "license": "MIT"
}
